<script type="text/javascript">
$(document).ready(function(){
    for(var i=1; i<=4; i++) {
        var img = new Image();
		img.src = 'images/project-'+i+'.jpg';
    }
    var image_index = 0;
    function changeProjectPageBackgound() {
        var image = ++image_index > 4 ? (image_index=1) : image_index;
        var el =  $('.header-project');
         el.fadeOut(500, function () {
             el.css('background-image', 'url("images/project-' + image +'.jpg")');
             el.fadeIn(500);
        });
    }
    changeProjectPageBackgound();
    setInterval(changeProjectPageBackgound, 8000);
});
</script>
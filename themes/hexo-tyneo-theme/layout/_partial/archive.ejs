<% if (pagination == 2){ %>
  <% page.posts.each(function(post){ %>
    <%- partial('article', {post: post, index: true}) %>
  <% }) %>
<% } else { %>
  <% var last; %>
  <% page.posts.each(function(post, i){ %>
    <% var year = post.date.year(); %>
    <% if (last != year){ %>
      <% if (last != null){ %>
        </div></section>
      <% } %>
      <% last = year; %>
      <section class="archives-wrap">
        <div class="archive-year-wrap">
          Tous les articles publi√©s en <a href="<%- url_for(config.archive_dir + '/' + year) %>" class="archive-year"><%= year %></a> :
        </div>
        <div class="archives">
    <% } %>
    <article><%- partial('_partial/article', {post: post, index: true}) %></article>
  <% }) %>
  <% if (page.posts.length){ %>
    </div></section>
  <% } %>
<% } %>
<% if (page.total > 1){ %>
  <div id="page-nav">

  </div>
<% } %>